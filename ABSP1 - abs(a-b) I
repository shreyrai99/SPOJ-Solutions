#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    ll t,n,i,j;
    cin>>t;
    while(t--)
    {
       cin>>n;
       ll a[n],s=0;
       for(i=0;i<n;i++)
        {
            cin>>a[i];
            s=s+(a[i]*i)-(a[i]*(n-1-i));
        }
        cout<<s<<"\n";
 
 
    }
    return 0;
}
